  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "coffeescript": "^2.5.0",
    "eslint": "^7",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-coffee": "^0.1.12",
    "eslint-plugin-prettier": "^3.1.4",
    "mocha": "^5.2.0",
